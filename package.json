{
  "name": "pixelity",
  "version": "1.0.0",
  "main": "src/index.js",
  "license": "MIT",
  "scripts": {
    "eslint": "eslint",
    "build": "webpack --config webpack.prod.babel.js",
    "compile": "babel src -d lib",
    "serve": "http-server ./build --cors -p 4000",
    "train:classifier": "node ./extra/node/text-classification/index.js",
    "train:entities": "node --max_old_space_size=8000 ./extra/node/entityclassifier/index.js ",
    "train:sketcher": "node ./extra/node/sketch-code/index.js",
    "train:lstm": "node ./extra/node/lstm/index.js",
    "train:breed": "node ./extra/node/pixelpet/breed/index.js",
    "train:knn": "node ./extra/node/pixelpet/breed/knn.js",
    "test:classifier": "node ./extra/node/text-classification/predict.js",
    "start": "webpack-dev-server --open --config webpack.dev.babel.js",
    "publish": "cp -r ./dist ./build/",
    "intents": "http-server ./public -p 4300"
  },
  "dependencies": {
    "@tensorflow-models/knn-classifier": "^0.2.2",
    "@tensorflow-models/mobilenet": "^0.2.2",
    "@tensorflow-models/posenet": "^0.2.2",
    "@tensorflow/tfjs": "^0.13.2",
    "@tensorflow/tfjs-core": "^0.13.10",
    "@tensorflow/tfjs-node": "^0.1.19",
    "audio-analyser": "^1.0.2",
    "canvas": "^2.2.0",
    "chalk": "^2.4.1",
    "chrono-node": "^1.3.5",
    "cli-progress": "^2.1.1",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-node": "^8.0.0",
    "euclidean-distance": "^1.0.0",
    "events": "^3.0.0",
    "face-api.js": "^0.15.1",
    "figlet": "^1.2.1",
    "get-image-pixels": "^1.0.1",
    "get-pixels": "^3.3.2",
    "gsap": "^2.0.2",
    "handsfree": "^3.0.6",
    "inquirer": "^6.2.0",
    "jpeg-js": "^0.3.4",
    "lorca-nlp": "^1.0.12",
    "mathjs": "^5.2.3",
    "meyda": "^4.1.3",
    "moment": "^2.22.2",
    "natural-language-flows": "0.0.1",
    "node-fetch": "^2.3.0",
    "node-nlp": "^2.3.0",
    "readline": "^1.3.0",
    "require": "^2.4.20",
    "shelljs": "^0.8.2",
    "shuffle-array": "^1.0.1",
    "sox.js": "^2.0.1",
    "tfjs-npy": "0.0.3",
    "xml2json": "^0.11.2",
    "xmlhttprequest": "^1.8.0"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "babel": {
    "presets": [
      [
        "env"
      ]
    ],
    "plugins": [
      "transform-runtime",
      "transform-object-rest-spread"
    ]
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "6.26.0",
    "babel-loader": "7.1.4",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-register": "6.26.0",
    "commitizen": "2.9.6",
    "cz-conventional-changelog": "2.1.0",
    "eslint": "^4.18.2",
    "eslint-config-airbnb-base": "12.1.0",
    "eslint-loader": "2.0.0",
    "eslint-plugin-import": "2.9.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "extract-text-webpack-plugin": "4.0.0-beta.0",
    "ghooks": "2.0.2",
    "html-webpack-plugin": "^3.0.7",
    "jasmine-core": "3.1.0",
    "karma": "2.0.0",
    "karma-browserstack-launcher": "~1.3.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-jasmine": "1.1.1",
    "karma-mocha-reporter": "^2.2.5",
    "karma-safari-launcher": "1.0.0",
    "karma-webpack": "3.0.0",
    "npm-run-all": "4.1.2",
    "regenerator-runtime": "0.11.1",
    "rimraf": "2.6.2",
    "semantic-release": "15.0.2",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "webpack": "4.1.1",
    "webpack-cli": "2.0.10",
    "webpack-dev-server": "3.1.0",
    "webpack-merge": "^4.1.2"
  }
}
